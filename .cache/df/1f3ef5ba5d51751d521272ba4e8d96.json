n    \n}\n\n.menu-toggle{\n    font-weight: 700;\n    font-size: 30px;\n    display: none;\n    cursor: pointer;\n    \n}\n\nheader .logo{\n    Font-size: 20px;\n    Font-weight: 600;\n}\n\nheader .logo a{\n    Color: #000;\n}\n\nheader .logo span,\nfooter .logo span{\n    Font-size: 35px;\n    Margin-right: -3px;\n    Color: red;\n}\n\n\nnav{\n    Display: flex;\n    Align-items:center;\n}\n\nnav li{\n    List-style: none;\n    Padding: 0 10px;\n}\n\nnav li a{\n    Color: #000;\n}\n\n\n.search-bar-toggle img{\n    Width: 18px;\n    cursor: pointer;\n}\n\n.search-bar-container{\n    Position: fixed;\n    Top: 55px;\n    Width:100%;\n    Background-color: rgba(0, 0, 0, 0.8);\n    height: 0;\n    overflow: hidden;\n    Border-bottom: 0px solid #000;\n    Transition: all 0.5s;\n}\n\n.search-bar-wrap{\n    Display:flex;\n    Align-items: center;\n    Justify-content: center;\n    Border-radius: 30px;\n    Background-color: #fff;\n    Width: 500px;\n    Margin: 30px auto;\n    Padding: 5px;\n}\n.search-bar-wrap input{\n    Padding: 5px 15px;\n    Background-color: transparent;\n    Border: 0;\n    Outline: 0;\n    flex: 1;\n}\n\n::placeholder{\n    Font-size: 16px;\n}\n.search-bar-wrap img{\n    Background-color: transparent;\n    Padding: 5px 15px;\n    Border: 0;\n    \n}\n\n.search-bar-container-toggle{\n    min-height: 100px;\n    z-index: 1000;\n}\n\n\n\n.user-toggle{\n    Position: relative;\n   Width: 150px;\n   Border: 0px solid #5f9341;\n  Margin-left: 90px;\n  cursor: pointer;\n}\n\n.toggle-user-profile{\n    Position: absolute;\n    Right: 0;\n    Left: 0;\n    Top: 45px;\n    padding: 10px 0;\n    Background-color: #000;\n    display: none;\n    Transition: all 0.5s;\n    \n}\n\n\n.toggle-user-profile a{\n    display: block;\n    Color: #fff;\n    Padding: 0 10px 10px 10px;\n}\n\n.toggled-user-profile{\n    display: inline-block;\n}\n\n@media (max-width: 900px){\n    \n    header{\n        display: block;\n    }\n   .menu-toggle{\n       Float: left;\n       Margin-Right: 20px;\n       display: inline-block;\n       Width: 30px;\n   }\n  \n  header .logo{\n       display: inline-block;\n       Width: 70%;\n       \n   }\n\n   header .logo{\n       font-size: 18px;\n       text-align: center;\n   }\n\n   header .logo span,\n   footer .logo span{\n       font-size: 25px;\n   }\n   \n header .search-bar-toggle{\n      float: right;\n      display: inline-block;\n      Text-align: center;\n      Margin-top: 8px;\n  }\n  \n  header nav{\n      display: block;\n      Position: fixed;\n      Left: -100%;\n      Bottom: 0;\n      Top: 55px;\n      Background-color: rgba(0, 0, 0);\n      Min-width: 80%;\n      Padding: 20px 7%;\n      overflow-y: auto;\n      Transition: all 0.5s;\n  }\n  \n .header-nav-toggle{\n     left: 0;\n }\n  \n  header nav li{\n      Padding: 10px 5px;\n  }\n  \n  header nav li a{\n      Color: #cecece;\n      \n  }\n  \n .user-toggle{\n     Margin-left: 0;\n     Color: #cecece;\n     Padding: 10px 5px;\n }\n\n .search-bar-wrap{\n     width: 70%;\n }\n  \n}"},"lineCount":208}},"error":null,"hash":"a2d3cc718f0f9599d7e324599c15e815","cacheData":{"env":{}}}